        "cache-manager": "^3.4.0",
        "cache-manager-redis-store": "^2.0.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "copyfiles": "^2.4.1",
        "dotenv": "^16.0.3",
        "firebase-admin": "^13.0.2",
        "generate-pincode": "^6.0.0",
        "libphonenumber-js": "^1.10.13",
